+					BR.xCoord = segmentOrigin.xCoord + right.xCoord * segmentHalfWidth;
+					BR.yCoord = segmentOrigin.yCoord + right.yCoord * segmentHalfWidth;
+					BR.zCoord = segmentOrigin.zCoord + right.zCoord * segmentHalfWidth;
